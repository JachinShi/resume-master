<div class="education_column">
    {{#each personal_info}}
        <p class="border_line">
            <span class="schoolName">{{school_name}}</span>
        </p>
        <span class="selfIntro">个人简介</span>
        <ul>
            <li>
                {{major}}
                <span class="type">{{type}}</span>
                <span>{{jiguan}}：{{native}}</span>
                <span class="now">{{xianju}}：{{now_place}}</span>
            </li>
            <li>
                {{enlevel}}  <span class="level">{{English_level}}</span>
                <span>{{xingbie}}：{{sex}}<span class="date">{{riqi}}：{{date_of_birth}}</span></span>
            </li>
            <li>
                <span class="desc">{{desc}}</span>
                <span>{{aihao}}：{{interests}}</span>
            </li>
        </ul>
    {{/each}}
</div>